{
  "name": "@drykiss/industry-ui",
  "description": "DryKISS React components library for NextJS",
  "author": "The IndustryUI Authors (https://github.com/DryKISS/industryui.com/graphs/contributors)",
  "contributors": ["DryKISS, Ltd."],
  "homepage": "https://industryui.com",
  "main": "dist/industry-ui.js",
  "module": "dist/industry-ui.es.js",
  "license": "MIT",
  "version": "1.0.0",
  "version_short": "0.1",
  "keywords": [
    "react",
    "nextjs",
    "components",
    "library",
    "mobile-first",
    "responsive",
    "front-end",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DryKISS/industryui.com.git"
  },
  "bugs": {
    "url": "https://github.com/DryKISS/industryui.com/issues"
  },
  "scripts": {
    "check": "npm-check -s -u",
    "serve": "npx http-server storybook-static",
    "start": "start-storybook -s ./.storybook/static -p 9000 --quiet -c .storybook",
    "standard": "standard | snazzy",
    "standard-fix": "standard --fix --verbose | snazzy",
    "prettier": "prettier-standard --staged --lint",
    "stylelint": "stylelint './components/**/*.js'",
    "test": "yarn run prettier && yarn run stylelint",
    "clean-dist": "rm -rf ./dist",
    "build": "NODE_ENV=build rollup -c",
    "build-storybook": "build-storybook --quiet -s ./.storybook/static",
    "test:snapshot": "jest",
    "test:snapshot:update": "jest --updateSnapshot"
  },
  "engines": {
    "node": ">=12"
  },
  "eslint": {
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "lint-staged": {
    "**/*.js": ["yarn test"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 100,
    "prettier.semi": false
  },
  "standard": {
    "globals": ["__SITE__"],
    "ignore": ["node_modules/**", "*.bundle*.js", "**/__tests__/*.test.js"],
    "parser": "babel-eslint"
  },
  "dependencies": {
    "@apollo/client": "^3.2.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.20",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@fullcalendar/core": "4.4.2",
    "@fullcalendar/daygrid": "4.4.2",
    "@fullcalendar/interaction": "4.4.2",
    "@fullcalendar/list": "4.4.2",
    "@fullcalendar/react": "4.4.2",
    "@fullcalendar/timegrid": "4.4.2",
    "@hookform/resolvers": "^0.1.1",
    "@nivo/bar": "^0.62.0",
    "@nivo/colors": "^0.62.0",
    "@nivo/generators": "^0.62.0",
    "@nivo/line": "^0.62.0",
    "@nivo/pie": "^0.62.0",
    "@tanem/react-nprogress": "^3.0.45",
    "@tippyjs/react": "^4.2.0",
    "@typeform/embed": "^0.25.1",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.16.1",
    "draft-js": "^0.11.7",
    "emoji-mart": "^3.0.0",
    "gravatar": "^1.8.1",
    "holderjs": "^2.9.7",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.0",
    "next": "^9.5.3",
    "react-big-calendar": "^0.28.0",
    "react-datepicker": "^3.2.2",
    "react-dropzone": "^11.2.0",
    "react-dropzone-uploader": "^2.11.0",
    "react-google-maps": "^9.4.5",
    "react-gtm-module": "^2.0.11",
    "react-hook-form": "^6.8.6",
    "react-resize-detector": "^5.2.0",
    "react-select": "^3.1.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@storybook/addon-a11y": "^6.0.22",
    "@storybook/addon-actions": "^6.0.22",
    "@storybook/addon-controls": "^6.0.22",
    "@storybook/addon-docs": "^6.0.22",
    "@storybook/addon-knobs": "^6.0.22",
    "@storybook/addon-storyshots": "^6.0.22",
    "@storybook/react": "^6.0.22",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.11.1",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "25.1.0",
    "jest-styled-components": "^7.0.2",
    "lint-staged": "^10.1.3",
    "markdown-loader-jest": "^0.1.1",
    "npm-check": "^5.9.2",
    "path": "^0.12.7",
    "prettier-standard": "^16.2.1",
    "react-test-renderer": "^16.13.1",
    "rollup": "^2.18.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^6.1.0",
    "snazzy": "^8.0.0",
    "standard": "^14.3.4",
    "storybook-readme": "^5.0.8",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1"
  },
  "peerDependencies": {
    "react": "17.0.0-rc.2",
    "react-dom": "17.0.0-rc.2",
    "styled-components": "^5.*"
  }
}
