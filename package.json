  "description": "DryKISS React components library for NextJS",
  "author": "The IndustryUI Authors (https://github.com/DryKISS/industryui.com/graphs/contributors)",
  "contributors": [
    "DryKISS, Ltd"
  ],
  "homepage": "https://industryui.com",
  "module": "esm/index.js",
  "files": [
    "esm"
  ],
  "sideEffects": false,
  "license": "MIT",
  "keywords": [
    "react",
    "nextjs",
    "components",
    "library",
    "mobile-first",
    "responsive",
    "front-end",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DryKISS/industryui.com.git"
  },
  "bugs": {
    "url": "https://github.com/DryKISS/industryui.com/issues"
  },
  "scripts": {
    "start": "start-storybook -s ./static -p 9000 --quiet -c .storybook",
    "serve": "npx http-server storybook-static",
    "test": "npx eslint . --ext .js",
    "test:fix": "npx eslint . --ext .js --fix",
    "clean": "rm -rf ./esm",
    "deploy-storybook": "storybook-to-ghpages --existing-output-dir=storybook-static",
    "build": "npm run clean && cross-env BABEL_ENV=esm babel components --out-dir esm  --source-maps false",
    "build-storybook": "build-storybook --quiet -s ./static",
    "pack": "npm run build && npm pack"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "storybook-deployer": {
    "gitUsername": "iwarner",
    "gitEmail": "ian.warner@drykiss.com",
    "commitMessage": "Deploy Storybook"
  },
