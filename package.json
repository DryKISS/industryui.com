{
  "name": "@drykiss/industry-ui",
  "description": "DryKISS React components library for NextJS",
  "author": "The IndustryUI Authors (https://github.com/DryKISS/industryui.com/graphs/contributors)",
  "contributors": [
    "DryKISS, Ltd."
  ],
  "homepage": "https://industryui.com",
  "main": "dist/industry-ui.js",
  "jsnext:main": "dist/industry-ui.es.js",
  "module": "dist/industry-ui.es.js",
  "sideEffects": false,
  "license": "MIT",
  "version": "1.0.12",
  "version_short": "0.1",
  "keywords": [
    "react",
    "nextjs",
    "components",
    "library",
    "mobile-first",
    "responsive",
    "front-end",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DryKISS/industryui.com.git"
  },
  "bugs": {
    "url": "https://github.com/DryKISS/industryui.com/issues"
  },
  "scripts": {
    "serve": "npx http-server storybook-static",
    "start": "start-storybook -s ./.storybook/static -p 9000 --quiet -c .storybook",
    "standard": "standard | snazzy",
    "standard-fix": "standard --fix --verbose | snazzy",
    "prettier": "prettier-standard --staged --lint",
    "prettier-local": "prettier-standard --lint",
    "stylelint": "stylelint './components/**/*.js'",
    "test": "yarn run prettier && yarn run stylelint",
    "clean-dist": "rm -rf ./dist",
    "deploy-storybook": "storybook-to-ghpages --existing-output-dir=storybook-static",
    "build": "cross-env NODE_ENV=build rollup -c",
    "build-storybook": "build-storybook --quiet -s ./.storybook/static",
    "pack": "yarn run build && npm pack",
    "test:snapshot": "cross-env ENVIRONMENT=test jest",
    "test:snapshot:update": "cross-env ENVIRONMENT=test  jest --updateSnapshot"
  },
  "engines": {
    "node": ">=12"
  },
  "eslint": {
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
  },
  "prettier": {
    "printWidth": 100,
    "prettier.semi": false
  },
  "standard": {
    "globals": [
      "__SITE__",
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ],
    "ignore": [
      "dist/**",
      "node_modules/**",
      "*.bundle*.js",
      "**/__tests__/*.test.js",
      "**/__tests__/**.test.js"
    ],
    "parser": "babel-eslint"
  },
  "storybook-deployer": {
    "gitUsername": "iwarner",
    "gitEmail": "ian.warner@drykiss.com",
    "commitMessage": "Deploy Storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.2.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@fullcalendar/core": "4.4.2",
    "@fullcalendar/daygrid": "4.4.2",
    "@fullcalendar/interaction": "4.4.2",
    "@fullcalendar/list": "4.4.2",
    "@fullcalendar/react": "4.4.2",
    "@fullcalendar/timegrid": "4.4.2",
    "@hookform/resolvers": "^0.1.1",
    "@nivo/bar": "^0.62.0",
    "@nivo/colors": "^0.62.0",
    "@nivo/generators": "^0.62.0",
    "@nivo/line": "^0.62.0",
    "@nivo/pie": "^0.62.0",
    "@tanem/react-nprogress": "^3.0.45",
    "@tippyjs/react": "^4.2.0",
    "@typeform/embed": "^0.25.1",
    "audio-recorder-polyfill": "^0.4.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.16.1",
    "draft-js": "^0.11.6",
    "draft-js-emoji-plugin": "^2.1.3",
    "draft-js-hashtag-plugin": "^2.0.4",
    "draft-js-linkify-plugin": "^2.0.2",
    "draft-js-mention-plugin": "^3.1.5",
    "draft-js-plugins-editor": "^3.0.0",
    "draftjs-to-html": "^0.9.1",
    "emoji-mart": "^3.0.0",
    "gravatar": "^1.8.1",
    "highcharts": "^8.2.0",
    "highcharts-react-official": "^3.0.0",
    "holderjs": "^2.9.7",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.0",
    "next": "^10.0.3",
    "react-big-calendar": "^0.28.0",
    "react-color": "^2.18.1",
    "react-cropper": "^2.1.4",
    "react-datepicker": "^3.2.2",
    "react-draft-wysiwyg": "^1.14.5",
    "react-dropzone": "^11.2.0",
    "react-dropzone-uploader": "^2.11.0",
    "react-google-maps": "^9.4.5",
    "react-gtm-module": "^2.0.11",
    "react-h5-audio-player": "^3.5.0",
    "react-hook-form": "^6.9.4",
    "react-pdf": "^5.0.0",
    "react-resize-detector": "^5.2.0",
    "react-select": "^4.0.2",
    "react-virtualized": "^9.22.2",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-storyshots": "^6.1.15",
    "@storybook/react": "^6.1.15",
    "@storybook/storybook-deployer": "^2.8.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-env": "^7.0.2",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "26.4.2",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.4.0",
    "markdown-loader-jest": "^0.1.1",
    "path": "^0.12.7",
    "prettier-standard": "^16.4.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^16.13.1",
    "rollup": "^2.33.3",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-visualizer": "^4.2.0",
    "snazzy": "^8.0.0",
    "standard": "^14.3.4",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1"
  },
  "peerDependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "styled-components": "^5.*"
  }
}
