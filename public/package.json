{
  "name": "industry-ui",
  "author": "Ian Warner <ian.warner@drykiss.com>",
  "version": "0.0.1",
  "description": "React components library",
  "main": "components/index.js",
  "license": "MIT",
  "repository": "git@github.com:drykiss/industry-ui.git",
  "scripts": {
    "start": "start-storybook -p 9000 --quiet -c .storybook",
    "test": "jest --coverage",
    "clean-dist": "rm -rf ./dist",
    "build": "npm run clean-dist && NODE_ENV=production `yarn bin`/babel ./src --out-dir ./dist",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "react",
    "component",
    "library"
  ],
  "standard": {
    "globals": [
      "__SITE__"
    ],
    "ignore": [
      "node_modules/**",
      "*.bundle*.js"
    ],
    "parser": "babel-eslint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "@typeform/embed": "^0.5.12",
    "jquery": "^3.3.1",
    "next": "latest",
    "react-dropzone": "latest",
    "react-select": "latest",
    "react-slick": "^0.23.2",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.0.0",
    "@babel/template": "7.0.0",
    "@storybook/addon-a11y": "latest",
    "@storybook/addon-actions": "latest",
    "@storybook/addon-info": "latest",
    "@storybook/addon-knobs": "latest",
    "@storybook/addon-notes": "latest",
    "@storybook/addon-options": "latest",
    "@storybook/addon-storyshots": "latest",
    "@storybook/react": "latest",
    "@storybook/storybook-deployer": "latest",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.4.2",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-styled-components": "latest",
    "jest": "latest",
    "react": "latest",
    "react-dom": "latest",
    "standard": "latest",
    "storybook-readme": "latest",
    "styled-components": "latest",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1"
  },
  "peerDependencies": {
    "react": "latest",
    "styled-components": "latest"
  }
